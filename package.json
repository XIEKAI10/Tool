{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "cross-env UMI_ENV=dev umi build",
    "product": "cross-env UMI_ENV=product umi build",
    "test": "umi test",
    "lint": "eslint {src,mock,tests}/**/*.{ts,tsx} --fix",
    "precommit": "lint-staged",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint:style": "stylelint src/**/*.{less,css} --syntax less"
  },
  "dependencies": {
    "@types/react-color": "^3.0.4",
    "@umijs/preset-react": "^1.6.10",
    "antd": "^4.1.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "caniuse-lite": "^1.0.30001219",
    "dva": "^2.6.0-beta.6",
    "react": "^16.13.1",
    "react-color": "^2.19.3",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^10.0.0",
    "@commitlint/config-conventional": "^10.0.0",
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "antd-theme-generator": "^1.2.10",
    "antd-theme-webpack-plugin": "^1.3.9",
    "babel-eslint": "^9.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^4.2.5",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "umi": "^3.2.23",
    "umi-request": "^1.3.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "git add"
    ],
    "*.{js,jsx}": [
      "git add"
    ],
    "**/*.{less,css}": "npm run lint:style"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
